                    Actual := TestInstancePathNameWith253CharsResult,
                    'Test instance path not correct');

AssertEquals_STRING(Expected := ShortTestMessageExpectedResult,
                    Actual := ShortTestMessageResult,
                    'Test message not correct');

TEST_FINISHED();]]></ST>
      </Implementation>
    </Method>
